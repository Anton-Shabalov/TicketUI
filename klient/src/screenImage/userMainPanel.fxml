        <Button fx:id="commandLine" layoutX="1.0" layoutY="120.0" mnemonicParsing="false" onAction="#clickButtonCommandPanel" opacity="0.79" prefHeight="69.0" prefWidth="799.0" style="-fx-background-color: #d6f4ff#d6f4ff;" text="Командная строка">
            <font>
                <Font name="Arial" size="26.0" />
            </font>
        </Button>
        <Button fx:id="TeblePage" layoutY="199.0" mnemonicParsing="false" onAction="#clickButtonColumn" opacity="0.79" prefHeight="69.0" prefWidth="800.0" style="-fx-background-color: #d6f4ff#d6f4ff;" text="Таблица с элементами">
            <font>
                <Font size="26.0" />
            </font>
        </Button>
      <Button fx:id="Visual" layoutY="277.0" mnemonicParsing="false" onAction="#clickButtonVisual" opacity="0.79" prefHeight="69.0" prefWidth="800.0" style="-fx-background-color: #d6f4ff#d6f4ff;" text="Визуализация элементов">
